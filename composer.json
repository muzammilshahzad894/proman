{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1.0",
        "ext-curl": "*",
        "authorizenet/authorizenet": "~1.9.6",
        "barryvdh/laravel-dompdf": "^2.0",
        "braintree/braintree_php": "^6.10",
        "bugsnag/bugsnag-laravel": "^2.25",
        "darkaonline/l5-swagger": "^8.4",
        "doctrine/dbal": "^3.5",
        "drewm/mailchimp-api": "^2.5",
        "globalpayments/php-sdk": "^6.0",
        "guzzlehttp/guzzle": "^7.5",
        "intervention/image": "^2.7",
        "laravel/framework": "^10.0",
        "laravel/helpers": "^1.5",
        "laravel/passport": "^11.3",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.1",
        "lcobucci/jwt": "^4.2",
        "maatwebsite/excel": "^3.1",
        "psr/simple-cache": "^2.0",
        "sentry/sentry-laravel": "^3.1",
        "stripe/stripe-php": "^10.1",
        "symfony/http-client": "^6.2",
        "symfony/mailgun-mailer": "^6.2",
        "twilio/sdk": "^6.43",
        "venturecraft/revisionable": "^1.39",
        "zircote/swagger-php": "^4.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
